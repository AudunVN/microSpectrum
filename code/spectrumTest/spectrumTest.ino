uint8_t randNumber;

void setup() {   
  delay(100);
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // ...and initialize with the I2C addr 0x3C
  display.clearDisplay();
  randomSeed(analogRead(0));
  display.setTextColor(1);
  display.setTextSize(1);
}

bool ledState = true;


void loop(void) {
    randNumber = random(128);
    display.clearDisplay();
    display.setCursor(0,0);
    display.drawRect(0, 0, 96, 11, GLC_BLUE);
    /*display.fillRect(1, 1, 94, 9, WHITE);*/
    display.drawRect(0, 10, 96, 53, GLC_BLUE);
    /*display.fillRect(1, 11, 94, 62, BLACK);*/
    for (int i = 2; i < 93; i = i+2) {
      int randLength = random(36);
      display.drawFastVLine(i, 61-randLength, randLength, GLC_CYAN);
      display.drawFastVLine(i+1, 61-randLength, randLength, GLC_CYAN);
    }
    display.setCursor(2,2); 
    display.print("space@471.no");
    display.setCursor(77,2);
    display.print("99%");
    display.setCursor(2,12);
    /* overlay SPACE and Glaceon */
    uint8_t height = 25;
    uint8_t width = 29;
    int row = 0;
    int col = 0;
    int bufferIndex = 0;
    for (row = 0; row < height; row++) {
      for (col = 0; col < width; col++) {
        uint16_t pixel = pgm_read_word(glcSleep + bufferIndex);
        if (pixel != 6904) {
          if (pixel == 0) {
            display.drawPixel(64+col, 35+row, 0);
          } else {
            display.drawPixel(64+col, 35+row, 1);
          }
          /*display.print(pixel);
          display.print(" ");*/
        }
        bufferIndex += sizeof(uint16_t);
      }
    }

    height = 17;
    width = 88;
    row = 0;
    col = 0;
    bufferIndex = 0;
    for (row = 0; row < height; row++) {
      for (col = 0; col < width; col++) {
        uint16_t pixel = pgm_read_word(space + bufferIndex);
        if (pixel != 6904) {
          if (pixel == 0) {
            display.drawPixel(4+col, 14+row, 0);
          } else {
            display.drawPixel(4+col, 14+row, 1);
          }
        }
        bufferIndex += sizeof(uint16_t);
      }
    }
    ledState = !ledState;
    display.drawPixel(75, 5, ledState);    
    display.display();
}

